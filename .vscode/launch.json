{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach",
      "type": "coreclr",
      "request": "attach",
      "pipeTransport": {
        "pipeProgram": "docker",
        "pipeArgs": ["exec", "-i", "gradinware-server-dev"],
        "debuggerPath": "/vsdbg/vsdbg",
        "pipeCwd": "${workspaceRoot}",
        "quoteArgs": false
      },
      "sourceFileMap": {
        "/app": "${workspaceFolder}/src/core"
      }
    }
  ]
}
